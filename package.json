{
  "name": "itd-electron",
  "version": "1.0.0",
  "description": "A minimal Electron application",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build-linux": "electron-packager ./ --out ./dist --platform=linux --arch=x64 --overwrite",
    "build-window": "electron-packager ./ --out ./dist --platform=win32 --arch=x64 --overwrite"
  },
  "repository": "https://github.com/electron/electron-quick-start",
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial",
    "demo"
  ],
  "author": "GitHub",
  "license": "CC0-1.0",
  "devDependencies": {
    "electron": "^6.0.1",
    "electron-packager": "^14.0.4"
  }
}
